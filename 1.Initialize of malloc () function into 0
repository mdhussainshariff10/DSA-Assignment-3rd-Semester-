#include <stdio.h> 
#include <stdlib.h> 
 
int main() 
{ 
    int n = 5; 
    int *a = (int *)malloc(n * sizeof(int)); 
 
    for (int i = 0; i < n; i++) 
        a[i] = 0; 
 
    printf("Array elements are:\n"); 
    for (int i = 0; i < n; i++) 
        printf("%d ", a[i]); 
 
    free(a); 
    system("getmac"); 
    return 0; 
}
