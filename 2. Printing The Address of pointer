#include <stdio.h> 
 
int main() 
{ 
    int a; 
    int *p; 
 
    printf("Enter a value: "); 
    scanf("%d", &a); 
 
    p = &a; 
 
    printf("Value entered = %d\n", a); 
    printf("Address of a = %p\n", &a); 
    printf("Address stored in pointer p = %p\n", p); 
    printf("Address of pointer p = %p\n", &p); 
     system("getmac"); 
    return 0; 
}
