#include <stdio.h> 
#include <stdlib.h> 
 
struct node 
{ 
    int data; 
    struct node *next; 
}; 
 
int main() 
{ 
    struct node *top = NULL, *temp; 
    int ch, x; 
 
    while (1) 
    { 
        printf("\n1 Push\n2 Pop\n3 Display\n4 Exit\n"); 
        scanf("%d", &ch); 
 
        if (ch == 1) 
        { 
            temp = malloc(sizeof(struct node)); 
            printf("Enter value: "); 
            scanf("%d", &temp->data); 
            temp->next = top; 
            top = temp; 
        } 
        else if (ch == 2) 
        { 
            if (top == NULL) 
                printf("Stack Empty"); 
            else 
            { 
                temp = top; 
                printf("Popped: %d", temp->data); 
                top = top->next; 
                free(temp); 
            } 
        } 
        else if (ch == 3) 
        { 
            temp = top; 
            if (temp == NULL) 
                printf("Stack Empty"); 
            else 
                while (temp != NULL) 
                { 
                    printf("%d ", temp->data); 
                    temp = temp->next; 
                } 
        } 
        else 
            break; 
    } 
    system("getmac"); 
 
    return 0; 
}
