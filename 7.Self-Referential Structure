#include <stdio.h> 
 
struct node 
{ 
    int data; 
    struct node *next; 
}; 
 
int main() 
{ 
    struct node a, b; 
 
    printf("Enter value for first node: "); 
    scanf("%d", &a.data); 
 
    printf("Enter value for second node: "); 
    scanf("%d", &b.data); 
 
    a.next = &b; 
    b.next = NULL; 
 
    printf("%d\n", a.data); 
    printf("%d\n", a.next->data); 
    system("getmac"); 
 
    return 0; 
}
